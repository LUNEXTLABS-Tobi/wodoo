# so that ./manage.sh attach {machinename} works: container_name == prefix + service-name
version: '2'

services:
  perftest:
    container_name: ${DCPREFIX}_perftest
    build: ../machines/perftest
    env_file: ../customs.env
    environment:
      DURATION_TO_OUTPUT: 2
      SLEEP: 10
      USERNAME: admin
      PASSWORD: 1
