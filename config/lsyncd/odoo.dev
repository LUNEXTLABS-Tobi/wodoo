settings {
	nodaemon = true,
	insist = true,
	maxDelays = 1,
} 
sync {default.rsyncssh,
	source = ".",
	host = "odoodev",
	targetdir = "/opt/odoo",
	exclude = { "dumps", ".git", "settings", "run" },
	rsync = {
		compress = true,
	},
}
sync {default.rsyncssh,
	source = "./run/debug",
	host = "nuc",
	targetdir = "/opt/odoo/run/debug",
	exclude = {  },
	rsync = {
		compress = true,
	},
}
