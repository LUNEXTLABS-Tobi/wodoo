<?xml version="1.0"?>
<odoo>
  <data noupdate="0">
    <record id="view_git_module_form" model="ir.ui.view">
      <field name="name">git.module form</field>
      <field name="model">git.module</field>
      <field name="arch" type="xml">
        <form string="" version="7.0">
          <header/>
          <sheet>
            <div class="oe_title">
              <h1>
                <field name="name" colspan="4" class="oe_inline"/>
              </h1>
            </div>
            <notebook>
              <page string="Branches">
                <group>
                  <field name="branch_id"/>
                  <field name="branch_ids">
                    <tree>
                      <field name="name"/>
                    </tree>
                  </field>
                </group>
              </page>
              <page string="Sub-Modules">
                <group string="" col="4">
                  <field name="module_ids" nolabel="1">
                    <tree>
                      <field name="name"/>
                    </tree>
                  </field>
                </group>
              </page>
            </notebook>
          </sheet>
        </form>
      </field>
    </record>
  </data>
</odoo>
